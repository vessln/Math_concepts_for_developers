def apply_rules(state, rules):
    next_state = ""
    for char in state:
        if char in rules:
            next_state += rules[char]
        else:
            next_state += char
    return next_state

def generate_system(axiom, rules, iterations):
    state = axiom
    for _ in range(iterations):
        state = apply_rules(state, rules)
    return state

